<div class="panel-group" id="accordion1">
  <div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" data-parent="#accordion1" href="#collapseOne">
            Spreadsheet Upload
          </a>
        </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse">
      <div class="panel-body">
        <form ng-submit="submitForm()" ng-controller="UploadCtrl" enctype="multipart/form-data">
          <%= label_tag 'assembly' %>
          <%= text_field_tag 'assembly', nil, :'ng-model' => 'assembly' %>
          <%= file_field_tag 'spreadsheet', :'ng-file-select' => 'onFileSelect($files)' %>
          <%= submit_tag 'Import', :class => 'btn btn-small btn-info pull-right' %>
        </form>
      </div>
    </div>
  </div>
</div>
